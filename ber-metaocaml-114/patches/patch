         let desc =
           { val_type = ty; val_kind = Val_reg;
-            val_attributes = [];
+            val_attributes = [Trx.attr_level !global_stage]; (* NNN *)
             val_loc = Location.none;
             val_uid = Uid.mk ~current_unit:(Env.get_unit_name ());
           }
